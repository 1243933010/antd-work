// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model User{
  userId    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  access String
  avatar String?
  password String
  // labels UserLabel[]
  labels Json?

}

// model UserLabel{
//   id Int @default(autoincrement()) @id
//   name String
//   user User @relation(fields: [userId],references: [userId])
//   userId Int
// }

model Work{
   id Int @default(autoincrement()) @id
   userId Int
   workType Int
   startTime String?
   endTime String?
   mounthTime String?
   tag Json?
   workList WorkList[]
}

model WorkTag{
   value Int @default(autoincrement()) @id
   tag String
   color String
}

model WorkList{
  id Int @default(autoincrement()) @id
  value String
  work Work @relation(fields: [workId], references: [id])
  workId Int
}